  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaoke1256.orders.search.dao.StoreDao">
	<resultMap type="com.xiaoke1256.orders.search.bo.Store" id="storeMap">
		<id property="storeNo" column="store_no"/>
